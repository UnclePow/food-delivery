<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="uestc.learning.mapper.StudentMapper">
	<select id="findManyStudentById"
		resultType="uestc.learning.entity.Student">
		select * from "student" where studentid=#{id}
	</select>
</mapper>